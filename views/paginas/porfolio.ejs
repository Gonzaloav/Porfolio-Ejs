        <link rel="stylesheet" href="/static/css/footer.css">  
        <link rel="stylesheet" href="/static/css/porfoliominiaturas.css">
    </head>
<body>
    <header>
        <%- include ('../componentes/ejs/nav') %> 
      </header>

      <main> 
        <ul>
            <% fotos.forEach(function(foto) { %>
