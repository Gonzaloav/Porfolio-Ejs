<main>

  <!-- Buscamos la galeria entre los diferentes Arrys -->

  <div class="Galeria">
    <% filas.forEach( function(fila) { %>

      <!-- Ahora que tenemos la galeria buscamos las fotos para hacer filas -->

      <div class="Fila">
        <% fila.forEach(function(foto) { %>
          <img src="/photos/<%= foto.file %>" alt="<%= foto.file %>">
          <% }); %>
      </div>
      <% }); %>
  </div>

  <!-- Colocamos las fotos en cada fila. -->
  <ul>
    <% fotos.forEach(function(foto) { %>
      <img src="/photos/<%= foto.file %>" alt="...">
      <li>
        <%= foto.id_foto %>
          <%= foto.galeria_foto %>
      </li>
      <% }); %>
  </ul>

</main>